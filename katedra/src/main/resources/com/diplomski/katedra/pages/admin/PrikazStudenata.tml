<html t:type="adminLayout" title="About katedra"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter"
      xmlns:j="tapestry-library:jquery">
<head>
    <h1>prikaz studenata</h1>
</head>
<body>
<t:form t:id="filterForm">
    <t:errors/>
    <select t:type="select" value="year" validate="required" model="years"/>
    <select t:type="select" value="selectedPredmet" validate="required" model="predmetSelectModel" encoder="predmetEncoder"/>
    <input type="submit" value="Prikazi"/>
</t:form>
<t:grid t:source="students" exclude="programId" row="currentStudent" t:add="student" rowsPerPage="10" reorder="student,brojBodova,konacnaOcena">
    <p:studentCell>${currentStudent.studentId.brojIndeksa}</p:studentCell>
